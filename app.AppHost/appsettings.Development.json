{
  "$schema": "https://json.schemastore.org/appsettings.json",
  // Logging configuration for the development environment
  "Logging": {
    // Log level settings control the minimum severity of messages to log
    "LogLevel": {
      // Default log level for all categories not explicitly configured
      "Default": "Information",
      // ASP.NET Core framework logs set to Warning to reduce noise
      "Microsoft.AspNetCore": "Warning"
    }
  }
}