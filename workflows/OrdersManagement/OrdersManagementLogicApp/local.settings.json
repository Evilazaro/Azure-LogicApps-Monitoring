{
  "$schema": "https://json.schemastore.org/local.settings.json",
  "IsEncrypted": false,
  "Values": {
    "APP_KIND": "workflowapp",
    "FUNCTIONS_WORKER_RUNTIME": "dotnet-isolated",
    "AzureWebJobsStorage": "UseDevelopmentStorage=true",
    "FUNCTIONS_INPROC_NET8_ENABLED": "1",
    "servicebus-ConnectionRuntimeUrl": "${SERVICE_BUS_CONNECTION_RUNTIME_URL}",
    "azureblob-ConnectionRuntimeUrl": "${AZURE_BLOB_CONNECTION_RUNTIME_URL}",
    "WORKFLOWS_TENANT_ID": "${AZURE_TENANT_ID}",
    "WORKFLOWS_SUBSCRIPTION_ID": "${AZURE_SUBSCRIPTION_ID}",
    "WORKFLOWS_RESOURCE_GROUP_NAME": "${AZURE_RESOURCE_GROUP}",
    "WORKFLOWS_LOCATION_NAME": "${AZURE_LOCATION}",
    "WORKFLOWS_MANAGEMENT_BASE_URI": "https://management.azure.com/",
    "WORKFLOWS_AUTHENTICATION_METHOD": "managedServiceIdentity"
  }
}